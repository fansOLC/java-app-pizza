<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.project.dao.Zpsd.ZpsdMapper">
	<select id="getZpsdfl" resultType="Zpsd">
		select * from zpsd
    </select>
    
    <select id="getZpsddl" parameterType="Zpsd" resultType="Zpsd">
		select * from zpsd where prefix=#{zpsdCode}
    </select>
    
    <select id="getZpsdxl" parameterType="Zpsd" resultType="Zpsd">
		select * from zpsd where prefix=#{zpsdCode}
    </select>
</mapper>  